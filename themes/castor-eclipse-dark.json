      "settings": {
        "foreground": "#ff5c7a",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["invalid.deprecated"],
      "settings": {
        "foreground": "#ffc757",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["meta.diff", "meta.diff.header"],
      "settings": {
        "foreground": "#858a99"
      }
    },
    {
      "scope": ["markup.inserted"],
      "settings": {
        "foreground": "#71ffc8"
      }
    },
    {
      "scope": ["markup.deleted"],
      "settings": {
        "foreground": "#ff7a94"
      }
    },
    {
      "scope": ["markup.changed"],
