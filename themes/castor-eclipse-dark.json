  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#858a99",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["string", "string.quoted"],
      "settings": {
        "foreground": "#a8e6ff"
      }
    },
    {
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#71ffc8"
      }
    },
    {
      "scope": ["constant.numeric", "constant.language"],
      "settings": {
        "foreground": "#aa94dc"
      }
    },
    {
      "scope": ["constant.character", "constant.other"],
      "settings": {
        "foreground": "#aa94dc"
      }
    },
    {
      "scope": ["variable", "variable.other"],
      "settings": {
        "foreground": "#e8ecf2"
      }
    },
    {
      "scope": ["variable.language"],
      "settings": {
        "foreground": "#ff7a94",
