      }
    },
    {
      "scope": ["string", "string.quoted"],
      "settings": {
        "foreground": "#40a02b"
      }
    },
    {
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#fe640b"
      }
    },
    {
