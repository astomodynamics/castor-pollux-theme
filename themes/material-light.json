  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#5f6368",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["string", "string.quoted"],
      "settings": {
        "foreground": "#0F9D58"
      }
    },
    {
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#EA4335"
      }
    },
    {
      "scope": ["constant.numeric", "constant.language"],
      "settings": {
        "foreground": "#A142F4"
      }
    },
    {
      "scope": ["constant.character", "constant.other"],
      "settings": {
        "foreground": "#A142F4"
      }
    },
    {
      "scope": ["variable", "variable.other"],
      "settings": {
        "foreground": "#202124"
      }
    },
    {
      "scope": ["variable.language"],
      "settings": {
        "foreground": "#EA4335",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["variable.parameter"],
      "settings": {
        "foreground": "#F9AB00"
      }
    },
    {
      "scope": ["keyword", "storage.type"],
      "settings": {
        "foreground": "#A142F4",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["storage.modifier"],
      "settings": {
        "foreground": "#A142F4"
      }
    },
    {
      "scope": ["keyword.control"],
      "settings": {
        "foreground": "#EA4335",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#4285F4"
      }
    },
    {
      "scope": ["entity.name.function", "meta.function-call", "support.function"],
      "settings": {
        "foreground": "#4285F4"
      }
    },
    {
      "scope": ["entity.name.type", "entity.name.class", "support.type", "support.class"],
      "settings": {
        "foreground": "#F9AB00"
      }
    },
    {
      "scope": ["entity.other.inherited-class"],
      "settings": {
        "foreground": "#0F9D58"
      }
    },
    {
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#FBBC04"
      }
    },
    {
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#EA4335"
      }
    },
    {
      "scope": ["punctuation.definition.tag"],
      "settings": {
        "foreground": "#4285F4"
      }
    },
    {
      "scope": ["markup.heading"],
      "settings": {
        "foreground": "#4285F4",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.italic"],
      "settings": {
        "foreground": "#202124",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["markup.bold"],
      "settings": {
        "foreground": "#202124",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.inline.raw", "markup.fenced_code"],
      "settings": {
        "foreground": "#0F9D58"
      }
    },
    {
      "scope": ["markup.underline.link"],
      "settings": {
        "foreground": "#4285F4",
        "fontStyle": "underline"
      }
    },
    {
      "scope": ["markup.list"],
      "settings": {
        "foreground": "#FBBC04"
      }
    },
    {
      "scope": ["markup.quote"],
      "settings": {
        "foreground": "#5f6368",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["support.variable"],
      "settings": {
        "foreground": "#24C1E0"
      }
    },
    {
      "scope": ["support.constant"],
      "settings": {
        "foreground": "#A142F4"
      }
    },
    {
      "scope": ["invalid"],
      "settings": {
        "foreground": "#EA4335",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["invalid.deprecated"],
      "settings": {
        "foreground": "#F9AB00",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["meta.diff", "meta.diff.header"],
      "settings": {
        "foreground": "#5f6368"
      }
    },
    {
      "scope": ["markup.inserted"],
      "settings": {
        "foreground": "#34A853"
      }
    },
    {
      "scope": ["markup.deleted"],
      "settings": {
        "foreground": "#EA4335"
      }
    },
    {
      "scope": ["markup.changed"],
      "settings": {
        "foreground": "#FBBC04"
      }
    },
    {
      "scope": ["punctuation.section.embedded"],
      "settings": {
        "foreground": "#EA4335"
      }
    },
    {
      "scope": ["meta.import", "meta.export"],
      "settings": {
        "foreground": "#A142F4"
      }
    }
  ]
}
